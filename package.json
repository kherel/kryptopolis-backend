{
  "name": "kryptopolis-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec node --harmony ./index.js | bunyan -o short -l trace",
    "start": "NODE_ENV=production node --harmony ./index.js | ./node_modules/.bin/bunyan -o short -l trace",
    "w:start": "SET \"NODE_ENV=production\" && node --harmony ./index.js | ./node_modules/.bin/bunyan -o short -l trace",
    "lint": "eslint .",
    "console": "node --require babel-register ./tools/console.js",
    "tests": "NODE_ENV=test _mocha --opts .mocha ./**/*.spec.js",
    "test": "NODE_ENV=test nodemon --exec _mocha --timeout 100000000 --compilers js:babel-register --reporter list --colors --full-trace --require ./spec/support/index.js",
    "w:tests": "SET \"NODE_ENV=test\" && _mocha --opts .mocha ./**/*.spec.js",
    "w:test": "SET \"NODE_ENV=test\" && nodemon --exec _mocha --timeout 100000000 --compilers js:babel-register --reporter list --colors --full-trace --require ./spec/support/index.js",
    "jobs": "node --require babel-register ./app/jobs/index.js",
    "db:migrate": "migrate-mongo ./.mongo-migrate.js",
    "db:migration": "migrate-mongo -f ./.mongo-migrate.js up",
    "w:db:migration": "migrate-mongo ./.mongo-migrate.js && up",
    "db:seed": "node --require babel-register ./db/seeds.js",
    "db:drop": "node --require babel-register ./db/drop.js",
    "db:backup": "node --require babel-register ./db/mongo/backup.js",
    "db:restore": "node --require babel-register ./db/mongo/restore.js",
    "db:ico:drop": "node --require babel-register ./db/ico/drop.js",
    "db:ico:seed": "node --require babel-register ./db/ico/seed.js",
    "db:ico:parse:github": "node --require babel-register ./db/ico/parse/github.js",
    "db:ico:parse:facebook": "node --require babel-register ./db/ico/parse/facebook.js",
    "db:ico:parse:twitter": "node --require babel-register ./db/ico/parse/twitter.js",
    "db:ico:upload:cloudinary": "node --require babel-register ./db/ico/upload/cloudinary.js",
    "db:ico:sync:mailchimp": "node --require babel-register ./db/ico/sync/mailchimp.js",
    "db:ico:fill:title_url": "node --require babel-register ./db/ico/fill/title_url.js",
    "db:token:get": "node --require babel-register ./db/token/get.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kherel/kryptopolis-backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kherel/kryptopolis-backend/issues"
  },
  "homepage": "https://github.com/kherel/kryptopolis-backend#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-resolver": "^1.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.17.2",
    "bunyan": "^1.8.12",
    "casl": "^1.0.6",
    "cloudinary": "^1.9.1",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "factory-girl": "^5.0.2",
    "faker": "^4.1.0",
    "fb": "^2.0.0",
    "jsdom": "^11.7.0",
    "jsonapi-serializer": "^3.5.6",
    "jsonwebtoken": "^7.4.3",
    "locale": "^0.1.0",
    "mailgun-js": "^0.13.1",
    "md5": "^2.2.1",
    "migrate-mongo": "^2.2.1",
    "moment": "^2.20.1",
    "mongodb-backup": "^1.6.9",
    "mongodb-restore": "^1.6.2",
    "mongoose": "^4.13.7",
    "mongoose-unique-validator": "^1.0.6",
    "node-schedule": "^1.2.5",
    "passport": "^0.4.0",
    "passport-facebook-token": "^3.3.0",
    "passport-google-token": "^0.1.2",
    "passport-twitter-token": "^1.3.0",
    "ramda": "^0.25.0",
    "request": "^2.85.0",
    "rss-to-json": "^1.0.3",
    "superagent": "^3.8.2",
    "swagger-ui-express": "^2.0.13",
    "to-snake-case": "^1.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "chai": "^4.1.1",
    "chai-subset": "^1.6.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "mocha": "^3.5.3",
    "nock": "^9.1.4",
    "nodemon": "^1.11.0",
    "run-sequence": "^2.2.1",
    "sinon": "^4.1.2",
    "supertest": "^3.0.0",
    "timekeeper": "^2.0.0"
  }
}
